  "license": "MIT",
  "scripts": {
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:test:run-migration": "npm run typeorm migration:run -- -d typeorm/test/config.ts",
    "typeorm:test:create-migration": "npm run typeorm migration:create typeorm/test/migrations/$npm_config_name",
    "typeorm:generate:clean": "rimraf ./temp",
    "typeorm-build-config": "tsc --experimentalDecorators --emitDecoratorMetadata typeorm/test/config.ts --outDir ./temp",
    "typeorm:test:generate-migration": "npm run typeorm-build-config && typeorm migration:generate -d temp/typeorm/test/config.js typeorm/test/migrations/$npm_config_name && npm run typeorm:generate:clean"
  },
  "private": true,
  "dependencies": {
